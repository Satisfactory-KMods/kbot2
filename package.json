{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"db": "pnpm drizzle-kit generate --config \"drizzle.config.ts\"",
		"build": "concurrently \"bun run build:lint\" \"bun run build:tsc\" && bun run build:nuxt",
		"build:lint": "eslint . --cache",
		"lint:timing": "TIMING=1 eslint .",
		"lint": "TIMING=1 eslint .",
		"build:tsc": "nuxi typecheck",
		"build:nuxt": "NODE_OPTIONS=\"--max-old-space-size=8192\" nuxi build",
		"dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" nuxi dev",
		"prepare": "husky install",
		"postinstall": "nuxt prepare",
		"run-prettier": "npx prettier --write **/*.{js,ts,json,tsx,vue,cjs,mjs}",
		"hooks:commitlint": "commitlint --edit",
		"hooks:pre-commit": "npx prettier --write **/*.{js,ts,json,tsx,vue,cjs,mjs} && git add ."
	},
	"dependencies": {
		"@auth/core": "^0.40.0",
		"@auth/drizzle-adapter": "^1.2.0",
		"@kmods/drizzle-pg": "1.15.0",
		"@nuxt/schema": "^3.11.2",
		"@nuxtjs/mdc": "^0.6.1",
		"@sidebase/nuxt-auth": "^0.10.0",
		"@tailwindcss/typography": "^0.5.13",
		"@vueuse/components": "^10.10.0",
		"@vueuse/nuxt": "^10.10.0",
		"axios": "^1.7.2",
		"discord.js": "^14.15.3",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.31.0",
		"fuse.js": "^7.0.0",
		"graphql-request": "^7.0.1",
		"h3-formidable": "^1.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"moment-timezone": "^0.6.0",
		"next-auth": "4.21.1",
		"node-cron": "^3.0.3",
		"nuxt": "^3.11.2",
		"pinia": "^2.1.7",
		"postgres": "^3.4.4",
		"primeicons": "^7.0.0",
		"primevue": "^3.52.0",
		"semver": "^7.6.2",
		"typescript-eslint": "^7.13.0",
		"uuid": "^10.0.0",
		"vue": "^3.4.27",
		"vue-final-modal": "^4.5.4",
		"vue-router": "^4.3.3",
		"vue3-emoji-picker": "^1.1.8",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@modyfi/vite-plugin-yaml": "^1.1.0",
		"@nuxt/devtools": "^1.3.3",
		"@nuxt/image": "^1.7.0",
		"@nuxt/types": "^2.17.3",
		"@nuxtjs/color-mode": "^3.4.1",
		"@nuxtjs/eslint-config-typescript": "^12.1.0",
		"@nuxtjs/eslint-module": "^4.1.0",
		"@nuxtjs/tailwindcss": "^6.12.0",
		"@pinia-plugin-persistedstate/nuxt": "^1.2.0",
		"@pinia/nuxt": "^0.11.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^12.0.0",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^10.0.3",
		"@semantic-release/release-notes-generator": "^13.0.0",
		"@types/formidable": "^3.4.5",
		"@types/lodash": "^4.17.5",
		"@types/node-cron": "^3.0.11",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"concurrently": "^8.2.2",
		"eslint": "^9.4.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-unused-imports": "^4.0.0",
		"formidable": "^3.5.1",
		"glob": "^10.4.1",
		"husky": "^9.0.11",
		"jsonc-eslint-parser": "^2.4.0",
		"moment-duration-format": "^2.3.2",
		"nuxt-icon": "^0.6.10",
		"nuxt-primevue": "^3.0.0",
		"prettier": "^3.3.1",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.6.2",
		"sass": "^1.77.4",
		"semantic-release": "^23.0.8",
		"sharp": "0.32.6",
