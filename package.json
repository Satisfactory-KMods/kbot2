{
  "name": "kbot2",
  "version": "2.2.5",
  "private": true,
  "scripts": {
    "production": "NODE_ENV=production ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/src/index.ts",
    "devserver": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/src/index.ts",
    "dev": "concurrently -m 2 \"vite --host --port 3000\" \"nodemon\"",
    "build": "yarn build:lint && yarn build:web && yarn build:server",
    "build:lint": "yarn lint",
    "build:web": "vite build --mode production",
    "build:server": "tsc --project ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json",
    "lint": "concurrently -m 3 \"yarn lint:server\" \"yarn lint:shared\" \"yarn lint:react\"",
    "lint:server": "eslint ./server/src/**/*.{ts,tsx,js,jsx} --fix",
    "lint:shared": "eslint ./shared/**/*.{ts,tsx,js,jsx} --fix",
    "lint:react": "eslint ./src/**/*.{ts,tsx,js,jsx} --fix"
  },
  "dependencies": {
    "@kyri123/k-javascript-utils": "^0.3.0-build.14",
    "@kyri123/k-reactutils": "^0.2.9-build.35",
    "@sweetalert2/theme-dark": "^5.0.15",
    "@trpc/client": "^10.34.0",
    "@trpc/server": "^10.34.0",
    "compressing": "^1.9.0",
    "devalue": "^4.3.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "emoji-picker-react": "^4.4.9",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "flowbite": "^1.7.0",
    "flowbite-react": "^0.4.11",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.1",
    "react-select": "^5.7.4",
    "sass": "^1.63.6",
    "similarity": "^1.2.1",
    "sweetalert2": "^11.7.18",
    "sweetalert2-react-content": "^5.0.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/similarity": "^1.2.1",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "nodemon": "^3.0.1",
    "postcss-cli": "^10.1.0",
    "react-error-overlay": "6.0.11",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.0.4",
    "vite": "^4.4.4",
    "vite-plugin-eslint": "^1.8.1"
  }
}
