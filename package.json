{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"db": "pnpm drizzle-kit generate:pg --config \"drizzle.config.ts\"",
		"build": "concurrently \"bun run build:lint\" \"bun run build:tsc\" && bun run build:nuxt",
		"build:lint": "eslint --ext .ts,.vue . --cache",
		"lint:timing": "TIMING=1 eslint --ext .ts,.vue .",
		"lint": "TIMING=1 eslint --ext .ts,.vue .",
		"build:tsc": "nuxi typecheck",
		"build:nuxt": "NODE_OPTIONS=\"--max-old-space-size=8192\" nuxi build",
		"dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" nuxi dev",
		"prepare": "husky install",
		"postinstall": "nuxt prepare",
		"run-prettier": "npx prettier --write **/*.{js,ts,json,tsx,vue,cjs,mjs}",
		"hooks:commitlint": "commitlint --edit",
		"hooks:pre-commit": "npx prettier --write **/*.{js,ts,json,tsx,vue,cjs,mjs} && git add ."
	},
	"dependencies": {
		"@auth/core": "^0.29.0",
		"@auth/drizzle-adapter": "^0.9.0",
		"@kmods/drizzle-pg": "1.7.1",
		"@sidebase/nuxt-auth": "^0.7.1",
		"@vueuse/components": "^10.9.0",
		"@vueuse/nuxt": "^10.9.0",
		"discord.js": "^14.14.1",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.20.14",
		"fuse.js": "^7.0.0",
		"graphql-request": "^6.1.0",
		"h3-formidable": "^1.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"moment-timezone": "^0.5.45",
		"next-auth": "4.21.1",
		"node-cron": "^3.0.3",
		"nuxt": "^3.11.2",
		"pinia": "^2.1.7",
		"postgres": "^3.4.4",
		"primeicons": "^7.0.0",
		"primevue": "^3.51.0",
		"uuid": "^9.0.1",
		"vue": "^3.4.21",
		"vue-final-modal": "^4.5.4",
		"vue-router": "^4.3.0",
		"vue3-emoji-picker": "^1.1.8",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.2.2",
		"@commitlint/config-conventional": "^19.2.2",
		"@modyfi/vite-plugin-yaml": "^1.1.0",
		"@nuxt/devtools": "^1.1.5",
		"@nuxt/image": "^1.5.0",
		"@nuxt/types": "^2.17.3",
		"@nuxtjs/color-mode": "^3.4.0",
		"@nuxtjs/eslint-config-typescript": "^12.1.0",
		"@nuxtjs/eslint-module": "^4.1.0",
		"@nuxtjs/tailwindcss": "^6.11.4",
		"@pinia-plugin-persistedstate/nuxt": "^1.2.0",
		"@pinia/nuxt": "^0.5.1",
		"@types/lodash": "^4.17.0",
		"@types/node-cron": "^3.0.11",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"concurrently": "^8.2.2",
		"eslint": "<9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-unused-imports": "^3.1.0",
		"husky": "^9.0.11",
		"jsonc-eslint-parser": "^2.4.0",
		"moment-duration-format": "^2.3.2",
		"nuxt-icon": "^0.6.10",
		"nuxt-primevue": "^0.3.1",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"sass": "^1.74.1",
		"sharp": "0.32.6",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"yaml-eslint-parser": "^1.2.2"
	}
}
