{
	"name": "kbot2",
	"version": "2.3.0",
	"private": true,
	"scripts": {
		"production": "NODE_ENV=production ts-node  --project ./server/tsconfig.json -r tsconfig-paths/register ./server/src/index.ts",
		"devserver": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/src/index.ts",
		"dev": "concurrently -m 2 \"vite --host --port 3000\" \"nodemon\"",
		"build": "pnpm run build:lint && pnpm run build:web && pnpm run build:server",
		"build:lint": "pnpm run lint",
		"build:web": "vite build --mode production",
		"build:server": "tsc --project ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json",
		"lint": "concurrently -m 3 \"pnpm run lint:server\" \"pnpm run lint:shared\" \"pnpm run lint:react\"",
		"lint:server": "eslint ./server --fix",
		"lint:shared": "eslint ./shared --fix",
		"lint:react": "eslint ./src --fix",
		"prepare": "husky install"
	},
	"dependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@kyri123/k-javascript-utils": "^0.3.0-build.14",
		"@kyri123/k-reactutils": "^0.2.9-build.35",
		"@sweetalert2/theme-dark": "^5.0.15",
		"@trpc/client": "^10.44.1",
		"@trpc/server": "^10.44.1",
		"compressing": "^1.10.0",
		"devalue": "^4.3.2",
		"discord.js": "^14.14.1",
		"dotenv": "^16.3.1",
		"emoji-picker-react": "^4.5.16",
		"express": "^4.18.2",
		"express-fileupload": "^1.4.3",
		"flowbite": "^2.2.0",
		"flowbite-react": "^0.8.0",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0",
		"husky": "^8.0.3",
		"jsonwebtoken": "^9.0.2",
		"jwt-decode": "^4.0.0",
		"lodash": "^4.17.21",
		"mongoose": "^8.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.12.0",
		"react-markdown": "^9.0.1",
		"react-router-dom": "^6.20.0",
		"react-select": "^5.8.0",
		"sass": "^1.69.5",
		"similarity": "^1.2.1",
		"sweetalert2": "^11.10.1",
		"sweetalert2-react-content": "^5.0.7",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.6.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^11.1.0",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.2.4",
		"@semantic-release/release-notes-generator": "^12.1.0",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.1",
		"@types/express": "^4.17.21",
		"@types/express-fileupload": "^1.4.4",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.0.0",
		"@types/react": "^18.2.39",
		"@types/react-dom": "^18.2.17",
		"@types/similarity": "^1.2.3",
		"@vitejs/plugin-react": "^4.2.0",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"eslint": "^8.54.0",
		"eslint-config-react": "^1.1.7",
