		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"nodemon": "^3.0.2",
		"postcss-cli": "^10.1.0",
		"prettier": "^3.1.0",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"react-error-overlay": "6.0.11",
		"semantic-release": "^22.0.8",
		"tailwindcss": "^3.3.5",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.8.8",
		"typescript": "^5.3.2",
		"vite": "^5.0.4",
		"vite-plugin-eslint": "^1.8.1"
	}
}
