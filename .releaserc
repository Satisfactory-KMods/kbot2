{
	"branches": [
		"main"
	],
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"preset": "conventionalcommits",
				"releaseRules": [
					{
						"breaking": true,
						"release": "major"
					},
					{
						"revert": true,
						"release": "patch"
					},
					{
						"type": "feat",
						"release": "minor"
					},
					{
						"type": "fix",
						"release": "patch"
					},
					{
						"type": "perf",
						"release": "patch"
					},
					{
						"tag": "BUGFIX",
						"release": "patch"
					},
					{
						"tag": "FEATURE",
						"release": "minor"
					},
					{
						"tag": "SECURITY",
						"release": "patch"
					},
					{
						"tag": "Breaking",
						"release": "major"
					},
					{
						"tag": "Fix",
						"release": "patch"
					},
					{
						"tag": "Update",
						"release": "minor"
					},
					{
						"tag": "New",
						"release": "minor"
					},
					{
						"component": "perf",
						"release": "patch"
					},
					{
						"component": "deps",
						"release": "patch"
					},
					{
						"type": "FEAT",
						"release": "minor"
					},
					{
						"type": "FIX",
						"release": "patch"
					},
					{
						"type": "refactor",
						"release": "patch"
					},
					{
						"type": "edit",
						"release": "patch"
					},
					{
						"type": "types",
						"release": false
					}
				]
			}
		],
		[
			"@semantic-release/release-notes-generator",
			{
				"preset": "conventionalcommits",
				"presetConfig": {
					"types": [
						{
							"type": "feat",
							"section": "Features",
							"hidden": false
						},
						{
							"type": "fix",
							"section": "Bug Fixes",
							"hidden": false
						},
						{
							"type": "refactor",
							"section": "Refactors",
							"hidden": false
						},
						{
							"type": "revert",
							"section": "Reverts",
							"hidden": false
						},
						{
							"type": "docs",
							"section": "Documentation",
							"hidden": false
						},
						{
							"type": "perf",
							"section": "Performance",
							"hidden": false
						},
						{
							"type": "edit",
							"section": "Adjustments",
							"hidden": false
						},
						{
							"type": "types",
							"hidden": true
						},
						{
							"type": "chore",
							"hidden": true
						}
					]
				},
				"writerOpts": {
					"commitsSort": [
						"subject",
						"scope"
					]
				}
			}
		],
		[
			"@semantic-release/changelog",
			{
				"changelogFile": "CHANGELOG.md",
				"changelogTitle": "# Changelog\n\nAll notable changes to this project will be documented in this file."
			}
		],
		[
			"@semantic-release/npm",
			{}
		],
		[
			"@semantic-release/github",
			{
				"assets": [
					"CHANGELOG.md"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		],
		[
			"@semantic-release/git",
			{
				"assets": [
					"CHANGELOG.md",
					"package.json"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	]
}
